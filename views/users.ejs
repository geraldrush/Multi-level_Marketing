<!-- views/users.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
    <h1>Users List</h1>
    <ul>
        <% users.forEach(user => { %>
            <li>
                <h2><%= user.name %></h2>
                <p>Email: <%= user.email %></p>
                <p>Role: <%= user.role %></p>
                <p>Location: <%= user.location %></p>
            </li>
        <% }) %>
    </ul>
</body>
</html>
